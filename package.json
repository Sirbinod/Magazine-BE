{
  "name": "magazine",
  "version": "1.0.0",
  "description": "magazine back-end(node, typescript, graphql and mongodb)",
  "main": "server.js",
  "scripts": {
    "start": "npm run transpile && node dist/server.js",
    "dev": "concurrently \"npm:watch\" \"npm:serve\"",
