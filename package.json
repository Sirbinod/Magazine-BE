    "watch": "tsc -w",
    "transpile": "tsc",
    "lint": "tslint -c tslint.json -p tsconfig.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Sirbinod/Magazine-BE.git"
  },
  "keywords": [
    "Tyscript",
    "mangoose",
    "node",
    "javascript"
  ],
  "author": "Sarbin Shrestha",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Sirbinod/Magazine-BE/issues"
  },
  "homepage": "https://github.com/Sirbinod/Magazine-BE#readme",
  "devDependencies": {
    "@types/express": "^4.17.1",
    "@types/node": "^16.11.6",
    "nodemon": "^2.0.14",
    "ts-node": "^10.4.0",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "@typegoose/typegoose": "^9.2.0",
    "@types/morgan": "^1.9.4",
    "apollo-server-core": "^3.5.0",
    "apollo-server-express": "^3.4.0",
    "class-validator": "^0.13.1",
    "concurrently": "^7.6.0",
    "dotenv": "^16.0.3",
    "express": "^4.17.1",
    "graphql": "^15.7.1",
    "helmet": "^6.0.1",
    "mongoose": "^6.0.12",
    "morgan": "^1.10.0",
    "reflect-metadata": "^0.1.13",
    "type-graphql": "^1.1.1"
